{
  "kind": "collectionType",
  "collectionName": "policies",
  "info": {
    "singularName": "policy",
    "pluralName": "policies",
    "displayName": "Policy",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Date_Submitted": {
      "required": true,
      "type": "date",
      "default": "2022-06-02"
    },
    "Date_Adjudicated": {
      "type": "date",
      "default": "2022-06-02"
    },
    "Committee_Status": {
      "type": "enumeration",
      "enum": [
        "In Review",
        "Pending Cabinet Decision",
        "Pending Committee Final Approval"
      ],
      "required": true,
      "default": "In Review"
    },
    "Cabinet_Decision": {
      "type": "enumeration",
      "enum": [
        "Approved",
        "Denied",
        "Pending Revisions"
      ]
    },
    "Member_1": {
      "displayName": "Member Vote",
      "type": "component",
      "repeatable": false,
      "component": "member.member-vote"
    },
    "Member_2": {
      "type": "component",
      "repeatable": false,
      "component": "member.member-vote"
    },
    "Purpose_Statement": {
      "type": "richtext"
    },
    "The_Policy": {
      "type": "richtext"
    },
    "Procedures": {
      "type": "richtext"
    },
    "Related_Policies_References": {
      "type": "richtext"
    },
    "Policy_History": {
      "type": "richtext"
    },
    "Appendix": {
      "type": "richtext"
    },
    "Category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "Audiences": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::audience.audience",
      "inversedBy": "policies"
    },
    "Cabinet_Approval": {
      "type": "component",
      "repeatable": false,
      "component": "member.member-vote"
    }
  }
}
